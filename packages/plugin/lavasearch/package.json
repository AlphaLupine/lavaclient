{
    "name": "@lavaclient/plugin-lavasearch",
    "version": "1.0.0",
    "description": "Lavalink API client plug-in for better integration with LavaSearch",
    "license": "Apache-2.0",
    "author": "lavaclient team",
    "homepage": "https://lavaclient.js.org",
    "repository": "git@github.com:lavaclient/lavaclient.git",
    "bugs": {
        "url": "https://github.com/lavaclient/lavaclient/issues"
    },
    "keywords": [
        "lavalink",
        "flexible",
        "easy",
        "performant",
        "lightweight",
        "music",
        "lavasearch",
        "youtube",
        "deezer",
        "spofify",
        "apple music"
    ],
    "maintainers": [
        {
            "email": "hi@vzt.gay",
            "name": "viztea",
            "url": "https://vzt.gay"
        }
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./register": {
            "import": "./dist/register.js",
            "require": "./dist/register.js",
            "types": "./dist/register.d.ts"
        }
    },
    "files": [
        "dist/*",
        "!dist/.tsbuildinfo"
    ],
    "scripts": {
        "build": "tsc",
        "check": "tsc --noEmit",
        "format": "prettier -w src/*",
        "prepublishOnly": "pnpm run format && pnpm run build"
    },
    "devDependencies": {
        "@lavaclient/config-prettier": "workspace:^",
        "@lavaclient/config-typescript": "workspace:^",
        "@types/node": "^20.3.3",
        "@types/ws": "^8.5.5",
        "typed-emitter": "^2.1.0",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "@effect/schema": "^0.49.4",
        "lavalink-api-client": "1.0.0",
        "lavalink-protocol": "1.0.0",
        "tslib": "^2.6.0"
    },
    "engines": {
        "node": ">=20"
    }
}