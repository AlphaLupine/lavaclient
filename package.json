{
  "name": "lavaclient",
  "version": "3.0.8",
  "description": "A simple and lightweight lavalink client written in typescript.",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "yarn build && node tests/index.ts",
    "build": "rimraf dist && tsc",
    "build:docs": "typedoc --theme ./node_modules/typedoc-neo-theme/bin/default",
    "prepublishOnly": "yarn build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "yarn lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lavaclient/lavaclient.git"
  },
  "keywords": [
    "lavalink",
    "lavaclient",
    "client",
    "discord",
    "modular",
    "lightweight",
    "flexible",
    "small",
    "easy"
  ],
  "author": "MeLike2D <https://github.com/melike2d>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lavaclient/lavaclient/issues"
  },
  "homepage": "https://github.com/lavaclient/lavaclient#readme",
  "dependencies": {
    "@kyflx-dev/lavalink-types": "^1.0.2",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.18",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "rimraf": "^3.0.2",
    "typedoc-neo-theme": "^1.0.8",
    "typescript": "^3.9.6"
  }
}
