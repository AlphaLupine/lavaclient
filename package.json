{
    "name": "lavaclient",
    "version": "4.1.0-rc.1",
    "description": "A simple, easy-to-use, and flexible lavalink client for node.js",
    "license": "Apache-2.0",
    "author": "lavaclient team <lavaclient@dimensional.fun>",
    "repository": "git@github.com:lavaclient/lavaclient.git",
    "bugs": {
        "url": "https://github.com/lavaclient/lavaclient/issues"
    },
    "exports": {
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**/*.{js,d.ts}"
    ],
    "maintainers": [
        {
            "url": "https://www.dimensional.fun",
            "name": "melike2d",
            "email": "ml2dofficial@gmail.com"
        }
    ],
    "scripts": {
        "clean": "rimraf dist",
        "build": "tsc",
        "lint": "dprint check && eslint src --ext js,ts",
        "format": "dprint fmt && eslint src --ext js,ts --fix"
    },
    "keywords": [
        "lavalink",
        "flexible",
        "easy",
        "performant",
        "lightweight"
    ],
    "dependencies": {
        "@lavaclient/types": "^2.1.0",
        "tiny-typed-emitter": "^2.1.0",
        "undici": "^5.8.0",
        "ws": "^8.2.3"
    },
    "devDependencies": {
        "@types/node": "^16.11.7",
        "@types/rimraf": "^3",
        "@types/ws": "^8.2.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "dprint": "^0.30.3",
        "eslint": "^8.20.0",
        "eslint-config-marine": "^9.4.1",
        "eslint-import-resolver-typescript": "^3.3.0",
        "eslint-plugin-import": "^2.26.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.7.4"
    },
    "engines": {
        "node": ">=16.x.x"
    },
    "packageManager": "yarn@3.2.1"
}
