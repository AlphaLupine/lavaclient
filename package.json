{
  "name": "lavaclient",
  "version": "3.0.0",
  "description": "A simple and lightweight lavalink client written in typescript.",
  "main": "build/index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "yarn build && node tests/index.ts",
    "build": "rimraf dist && tsc",
    "build:docs": "typedoc --theme ./node_modules/typedoc-neo-theme/bin/default",
    "prepublishOnly": "yarn build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "yarn lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lavaclient/lavaclient.git"
  },
  "keywords": [
    "simple",
    "lightweight",
    "modular",
    "lavalink",
    "client"
  ],
  "author": "MeLike2D",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lavaclient/lavaclient/issues"
  },
  "homepage": "https://github.com/lavaclient/lavaclient#readme",
  "dependencies": {
    "@kyflx-dev/lavalink-types": "^1.0.2",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.1",
    "@types/ws": "^7.2.5",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "rimraf": "^3.0.2",
    "typedoc-neo-theme": "^1.0.8",
    "typescript": "^3.9.2"
  }
}
